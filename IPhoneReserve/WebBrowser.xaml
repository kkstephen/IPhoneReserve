<Window x:Class="IPhoneReserve.WebBrowser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:IPhoneReserve"
        mc:Ignorable="d"
        Title="Web Browser" Height="568.783" Width="777.876" Loaded="Window_Loaded">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="27*"/>
            <RowDefinition Height="242*"/>
        </Grid.RowDefinitions>
        <WebBrowser x:Name="myIE" Margin="5" Grid.Row="1" Navigating="myIE_Navigating"/>
        <Button x:Name="btnGo" Content="Go" HorizontalAlignment="Left" Margin="5,15,0,0" VerticalAlignment="Top" Width="75" Height="20" Click="btnGo_Click"/>
        <TextBox x:Name="txtUrl" HorizontalAlignment="Left" Height="23" Margin="90,15,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="667"/>

    </Grid>
</Window>
